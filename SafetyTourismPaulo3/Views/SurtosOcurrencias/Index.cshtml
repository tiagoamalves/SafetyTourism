            <td>
                @Html.DisplayFor(modelItem => item.DataInicio)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Relatorio)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.SurtosEpidemiologico.SurtosEpidemiologicoID)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DestinoTuristico.DestinoTuristicoID)
            </td>
